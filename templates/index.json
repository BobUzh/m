    "2adc6ae4-ca08-427e-855e-a6dfd25d2559": {
      "type": "section-process",
      "blocks": {
        "030445d2-a8e8-4ba6-8ce9-6461ea31a5d7": {
          "type": "item",
          "settings": {
            "name": "",
            "img": "shopify:\/\/shop_images\/DSC05434_copy.jpg",
            "title": "Dedicated Ingredients",
            "subtitle": "In partnership with local farms and world-class chefs, we support your postpartum body and the local ecosystem by choosing organic, seasonal produce. The menu changes according to what’s growing near you. Our ingredients are chosen specifically to support each stage of your unique postpartum recovery."
          }
        },
        "8a4aace9-6b5c-43f1-b5ca-90da2763f86b": {
          "type": "item",
          "settings": {
            "name": "",
            "img": "shopify:\/\/shop_images\/20363215-F87C-4078-87FA-43FFEA3E8F1F_1_105_c_copy.webp",
            "title": "Designed for Nourishment",
            "subtitle": "Our menus include protein rich meals, porridges, soups, stews, lattes, teas, broths,  and lactation snacks designed to support digestion, nutrient assimilation, breast milk supply, hormone and mood balance, and overall physical recovery after birth. We utilize traditional cooking methods such as slow-cooking, soaking and sprouting nuts, seeds, and grains, and toasting and crushing whole spices. We cook with cast iron and clay and our kitchen is completely free from dairy, gluten, peanuts, and refined sugar."
          }
        },
        "571aa744-c444-4534-82af-e240adb7d156": {
          "type": "item",
          "settings": {
            "name": "",
            "img": "shopify:\/\/shop_images\/Screen_Shot_2023-04-28_at_9.24.25_AM.png",
            "title": "Delivered to Your Home",
            "subtitle": "We deliver on Sundays, Tuesdays, and Thursdays to guarantee optimal freshness throughout the week. Never frozen, and prepared each delivery day, our meals are packaged in reusable glassware and kept cool in a 100% biodegradable insulated liner and box. Our cold packs are drain-safe, and all materials that arrive in your first delivery can be reused throughout your entire meal plan. No prep or cooking necessary. Simply follow reheat instructions and enjoy (in bed if possible)."
          }
        }
      },
      "block_order": [
